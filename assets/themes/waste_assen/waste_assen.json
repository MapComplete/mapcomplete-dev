                "mappings": [
                  {
                    "if": "_has_closeby_recycling=yes",
                    "then": "circle:#008000aa"
                  },
                  {
                    "if": "_has_closeby_waste_disposal=yes",
                    "then": "circle:#008000aa"
                  }
                ]
              }
            }
          ],
          "iconSize": "20,20",
          "anchor": "center"
        }
      ],
      "lineRendering": []
