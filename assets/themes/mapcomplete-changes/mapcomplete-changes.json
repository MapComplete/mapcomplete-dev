          "freeform": {
            "key": "host"
          },
          "mappings": [
            {
              "if": "host=www.waldbrand-app.de",
              "then": "waldbrand-app.de",
              "hideInAnswer": true
            },
            {
              "if": "host~https://pietervdvn.github.io/mc/develop/.*",
              "then": "Develop",
              "hideInAnswer": true
            }
          ]
        },
